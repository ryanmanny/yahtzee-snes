{
    "version": 4,
    "env": {
        "pvsneslibPath": [
            "/root/code/snes/pvsneslib/pvsneslib/include/**",
            "/root/code/snes/pvsneslib/devkitsnes/include"
        ],
        "projectIncludePath": ["${workspaceFolder}", "${workspaceFolder}/src"]
      },
    "configurations": [
        {
            "name": "PVSneslib",
            "includePath": ["${projectIncludePath}", "${pvsneslibPath}"],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ]
}